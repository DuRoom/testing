{
    "name": "duroom/testing",
    "description": "Automated testing infrastructure for DuRoom core and extensions.",
    "keywords": [
        "forum",
        "discussion"
    ],
    "homepage": "https://duroom.js.org/",
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.0"
    },
    "require-dev": {
        "duroom/core": "*@dev"
    },
    "autoload": {
        "psr-4": {
            "DuRoom\\Testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DuRoom\\Testing\\Tests\\": "src/tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
